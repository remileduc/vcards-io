{
	"clearMocks": true,
	"collectCoverage": true,
	"collectCoverageFrom": [
		"lib/*.ts"
	],
	"transform": {
		"^.+\\.(ts|tsx)$": "ts-jest"
	},
	"moduleNameMapper": {
		"^(.+)\\.js$": "$1"
	},
	"coverageThreshold": {
		"global": {
			"branches": 80,
			"functions": 80,
			"lines": 80,
			"statements": 80
		}
	}
}
